{
  "manifest_version": 3,
  "name": "Bookmark Cat",
  "version": "1.6",
  "description": "Generate all your bookmarks with one click into a page that is easy to navigate and manage",
  "permissions": [
    "bookmarks",
    "storage",
    "downloads"
  ],
  "action": {
    "default_popup": "./popup.html"
    },
  "icons": {
    "16": "./img/icon.png",
    "48": "./img/icon.png",
    "128": "./img/icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}