{
  "manifest_version": 3,
  "name": "书签导出插件",
  "version": "1.1",
  "description": "一键导出书签为JSON或HTML预览。",
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
    },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}